[user]
	name = Ivan D Vasin
	email = nisavid@gmail.com
[core]
	autocrlf = input
	safecrlf = warn
	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = ~/.gitignore_global
	askpass = /usr/bin/ksshaskpass
[branch]
	autosetuprebase = always
[diff]
	algorithm = histogram
	tool = kompare
[grep]
	lineNumber = true
[log]
	date = iso
[merge]
	tool = kdiff3
[push]
	default = simple
[alias]
	a = add --patch
	aa = add --all
	br = branch
	brc = symbolic-ref --short HEAD
	cfg = config
	cfgg = config --global
	cfgge = config --global --edit
	cfgl = config --local
	cfgle = config --local --edit
	chp = cherry-pick
	chpa = cherry-pick --abort
	chpc = cherry-pick --continue
	ci = commit
	cia = commit --amend
	cian = commit --amend --no-edit
	cim = commit --message
	cin = commit --no-edit
	co = checkout
	co- = checkout -
	cob = checkout -b
	cogl = checkout HEAD -- Gemfile.lock
	coma = checkout master
	d = diff --find-copies --find-renames
	dc = diff --find-copies --find-renames --cached
	dcs = diff --find-copies --find-renames --cached --stat
	ds = diff --find-copies --find-renames --stat
	lgd = log --patch --find-copies --find-renames
	lgs = log --stat
	lgsd = log --stat --patch --find-copies --find-renames
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
	mg = merge
	mgf = merge --ff-only
	mgnf = merge --no-ff
	pl = pull
	ps = push
	psc = pushcur
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbima = rebase --interactive master
	rbiup = rebase --interactive @{upstream}
	rbma = rebase master
	rmc = rm --cached
	rs = reset
	s = status
	st = stash
	sta = stash apply
	stp = stash pop
	sts = stash show
	stsp = stash show --patch
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[gui]
	fontdiff = -family \"courier new\" -size 9 -weight normal -slant roman -underline 0 -overstrike 0
